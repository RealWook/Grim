#include <string>
#include <vector>

using namespace std;

int solution(vector<string> order) {
    int total_Price = 0;
    
       for (const string& item : order) 
       {
        if (item == "americano" || item == "iceamericano" || item == "hotamericano" || item == "anything" || 
            item.find("americano") == 0) 
        { 
            total_Price += 4500;
        } 
        else if (item == "cafelatte" || item == "icecafelatte" || item == "hotcafelatte" || 
                 item.find("cafelatte") == 0) 
        { 
            total_Price += 5000;
        }
        }
    return total_Price;
}
