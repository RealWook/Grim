#include <string>
#include <sstream>
#include <vector>
#include <stack>

using namespace std;

int solution(string s) {
    
    stack<int> st; // 최근 숫자를 저장하는 스택
    stringstream ss(s); // 스트림 사용 : 문자열을 쪼개거나 숫자로 변환할 수 있음
    string token;
    int sum = 0;
    
   while (ss >> token)  // 공백 기준으로 문자열 분리
   { 
        if (token == "Z") 
        {
            if (!st.empty()) 
            {
                st.pop(); // 가장 최근에 추가된 숫자 제거
            }
        } 
       else 
        {
            st.push(stoi(token)); // 숫자를 스택에 추가
        }
    }

       while (!st.empty()) // 스택 요소가 비어있지 않으면 계속 반복
       {
           sum += st.top();
           st.pop(); // 총 합에 더한 후, 요소 삭제
       }
    return sum;
}
